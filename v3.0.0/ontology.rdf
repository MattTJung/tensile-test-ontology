                                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IAO_0000032"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/OBI_0000312"/>
