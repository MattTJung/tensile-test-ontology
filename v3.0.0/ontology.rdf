        <obo:IAO_0000114 rdf:resource="https://w3id.org/pmd/tto"/>
        <obo:IAO_0000117>PERSON: Markus Schilling</obo:IAO_0000117>
        <obo:IAO_0000117>PERSON: Matthias Jung</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="de">DIN EN ISO 6892-1:2020-06</obo:IAO_0000119>
        <obo:IAO_0000119 xml:lang="en">ISO 6892-1:2019</obo:IAO_0000119>
        <rdfs:label xml:lang="en">Tensile Testing Machine (ISO 6892-1)</rdfs:label>
        <rdfs:label xml:lang="de">Zugprüfmaschine (ISO 6892-1)</rdfs:label>
        <skos:definition xml:lang="en">A tensile testing machine that is in accordance with ISO 6892-1:2019</skos:definition>
        <skos:definition xml:lang="de">Eine Zugprüfmaschine, die die Anforderungen aus ISO 6892-1:2019 erfüllt.</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/tto/ThicknessAfterFracture -->


